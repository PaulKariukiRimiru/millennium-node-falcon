{
  "name": "millennium-node-falcon",
  "version": "0.0.1",
  "description": "This is a custom node boilerplate project that combines node + express and various database systems.",
  "main": "src/index.ts",
  "dependencies": {
    "@types/mongodb": "^3.1.19",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fp-ts": "^1.13.0",
    "fs": "^0.0.1-security",
    "mongodb": "^3.1.13"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/supertest": "^2.0.7",
    "body-parser": "^1.18.3",
    "coveralls": "^3.0.2",
    "jest": "^24.0.0",
    "jest-environment-node": "^24.0.0",
    "mongodb-memory-server": "^3.0.0",
    "path": "^0.12.7",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.2.4"
  },
  "scripts": {
    "pretest:dev": "NODE_ENV=test node pre-dev-start.js",
    "pretest:coverage": "NODE_ENV=test node pre-dev-start.js",
    "prestart:dev": "NODE_ENV=dev node pre-dev-start.js",
    "test": "jest",
    "test:dev": "yarn test --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "start:dev": "tsnd --respawn --transpileOnly --project ./tsconfig.json src/Sample/index.ts",
    "lint": "tslint {src}/**/*.ts --fix",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulKariukiRimiru/millennium-node-falcon.git"
  },
  "keywords": [
    "boilerplate",
    "typescript",
    "node",
    "express"
  ],
  "author": "Paul Rimiru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PaulKariukiRimiru/millennium-node-falcon/issues"
  },
  "homepage": "https://github.com/PaulKariukiRimiru/millennium-node-falcon#readme",
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "node"
    ]
  }
}
